@model IEnumerable<TagDTO>
 
<div class="container">
    <div class="tags">
         <div class="tag">
             <div class="profile-link">
                 @foreach(var item  in @Model)
                    {
                        <div class="item">
                            <a href="@item.TargetLink" target="_blank" rel="noopener noreferrer">@item.Name</a>
                        
                        </div> 
                    }
            </div>            
         </div>
    </div>
</div>
