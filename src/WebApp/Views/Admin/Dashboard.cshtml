@model Dashboard


<input type="hidden" asp-for="@Model.Id" />
<input type="hidden" id="url" name="url" value="@Url.Action("DashboardDate", "Admin")" />


<h1>@Model.Tag.Name</h1>

<div class="container">
    <div class="dashboard">
        <div class="right">
            <div class="count">
                <h1>@Model.Qtd</h1>
            </div>
 
        </div>
        
    </div>
    <div>
        <div class="graph">
            <div id="chart_div" style="width: 100%; height: 500px;"></div>
        </div>
    </div>
</div>


@section Scripts
{
    

  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
  <script type="text/javascript" src="~/js/areaChart.js"></script>
   

}

